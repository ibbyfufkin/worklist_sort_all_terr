            Target6.Cells(j, "A").Value = Source.Cells(c.Row, "b").Value
            Target6.Cells(j, "b").Value = Source.Cells(c.Row, "c").Value
            Target6.Cells(j, "c").Value = Source.Cells(c.Row, "d").Value
            Target6.Cells(j, "d").Value = Source.Cells(c.Row, "e").Value
            Target6.Cells(j, "e").Value = Source.Cells(c.Row, "f").Value
            Target6.Cells(j, "f").Value = Source.Cells(c.Row, "g").Value
            Target6.Cells(j, "g").Value = Source.Cells(c.Row, "h").Value
