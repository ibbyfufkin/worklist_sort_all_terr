            Target7.Cells(j, "e").Value = Source.Cells(c.Row, "f").Value
            Target7.Cells(j, "f").Value = Source.Cells(c.Row, "g").Value
            Target7.Cells(j, "g").Value = Source.Cells(c.Row, "h").Value
            Target7.Cells(j, "h").Value = Source.Cells(c.Row, "i").Value
            Target7.Cells(j, "i").Value = Source.Cells(c.Row, "j").Value
