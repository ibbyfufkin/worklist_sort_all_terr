            Target7.Cells(j, "A").Value = Source.Cells(c.Row, "b").Value
            Target7.Cells(j, "b").Value = Source.Cells(c.Row, "c").Value
            Target7.Cells(j, "c").Value = Source.Cells(c.Row, "d").Value
