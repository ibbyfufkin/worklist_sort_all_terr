        If c = "3" Then
            Target3.Cells(j, "A").Value = Source.Cells(c.Row, "b").Value
            Target3.Cells(j, "b").Value = Source.Cells(c.Row, "c").Value
            Target3.Cells(j, "c").Value = Source.Cells(c.Row, "d").Value
            Target3.Cells(j, "d").Value = Source.Cells(c.Row, "e").Value
            Target3.Cells(j, "e").Value = Source.Cells(c.Row, "f").Value
            Target3.Cells(j, "f").Value = Source.Cells(c.Row, "g").Value
            Target3.Cells(j, "g").Value = Source.Cells(c.Row, "h").Value
            Target3.Cells(j, "h").Value = Source.Cells(c.Row, "i").Value
            Target3.Cells(j, "i").Value = Source.Cells(c.Row, "j").Value
            Target3.Cells(j, "j").Value = Source.Cells(c.Row, "k").Value
            Target3.Cells(j, "k").Value = Source.Cells(c.Row, "l").Value
            Target3.Cells(j, "l").Value = Source.Cells(c.Row, "m").Value
            j = j + 1
        End If
    Next c
    
    j = 5     ' Start copying to row 5 in target sheet
